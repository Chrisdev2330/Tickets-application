<!DOCTYPE html>
<html>
  <head>
    <title>AppTickets</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>

  </head>

  <body>
    <div id="nav">
    <% if current_user %>
      <%= current_user.username %>
      <%= link_to "Edit Profile", edit_profile_user_path(current_user.id) %>
      <%= link_to "Logout", :logout, data: { turbo_method: :post } %>
    <% else %>
      <%= link_to "Register", new_user_path %> |
      <%= link_to "Login", :login %>
    <% end %>
  </div>
  <div>
  </div>
    <%= yield %>
  </body>
</html>
